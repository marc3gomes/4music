<?xml version="1.0" encoding="UTF-8"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="formkey"/>
    <head>
    	<title>Testimonial</title>
    </head>
    <body>
    	<referenceBlock name="menu">
    	    <action method="setActive">
    	        <argument name="itemId" xsi:type="string">Blueskytechco_Testimonial::testimonial</argument>
    	    </action>
    	</referenceBlock>
    	<referenceBlock name="page.title">
    	    <action method="setPageTitle">
    	        <argument name="pageTitle" xsi:type="string">Testimonial</argument>
    	    </action>
    	</referenceBlock>
        <referenceContainer name="content">
            <block class="Blueskytechco\Testimonial\Block\Adminhtml\Testimonial" name="admin.block.testimonial.grid.container"/>
        </referenceContainer>
    </body>
</page>
